{
  "builds": [
    {
      "src": "api/**/*.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/files",
      "dest": "/api/files.go"
    },
    {
      "src": "/",
      "dest": "/api/index.go"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/re-index",
      "schedule": "0 0 * * *"
    }
  ]
}
